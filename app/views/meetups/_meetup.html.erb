<dl class="dl-horizontal">
  <dt>Title</dt>
  <dd><%= link_to meetup.title, meetup %></dd>

  <dt>Description</dt>
  <dd><%= meetup.description %></dd>

  <dt>Format</dt>
  <dd><%= meetup.format %></dd>
  
  <dt>Date</dt>
  <dd><%= meetup.date %></dd>

  <dt>Start</dt>
  <dd><%= meetup.start_time %></dd>

  <dt>End</dt>
  <dd><%= meetup.end_time %></dd>

  <% if current_user?(meetup.user) %>
    <dd><%= link_to "edit", meetup, method: :post %> | <%= link_to "delete", meetup, method: :delete, data: { confirm: "You sure?" } %></dd>
  <% end %>
</dl>
