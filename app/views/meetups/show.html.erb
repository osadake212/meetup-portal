<div class="row">
  <div class="col-lg-8">
    <div class="row">
      <div class="col-lg-12">
        <div class="panel-meetup">
          <%= render @meetup %>
        </div>
      </div>
    </div>
  </div>
  <div class="col-lg-4">
    <%= render 'attend_form' if signed_in? %>
    <h3>Attend Users</h3>
    <% if @meetup.attend_users.any? %>
      <% @meetup.attend_users.each do |user| %>
        <div><%= link_to user.name, user_path(user) %></div>
      <% end %>
    <% else %>
      <div>No user attend this meetup.</div>
    <% end %>
  </div>
</div>

