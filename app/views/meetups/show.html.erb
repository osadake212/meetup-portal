<div class="row">
  <div class="col-lg-8">
      <%= render @meetup %>
    </section>
  </div>
  <div class="col-lg-4">
    <%= render 'attend_form' if signed_in? %>
    <h3>Attend Users</h3>
    <% if @meetup.attend_users.any? %>
      <% @meetup.attend_users.each do |user| %>
        <div><%= user.name %></div>
      <% end %>
    <% else %>
      <div>No user attend this meetup.</div>
    <% end %>
  </div>
</div>

